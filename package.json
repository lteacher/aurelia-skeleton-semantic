{
  "name": "aurelia-skeleton-semantic",
  "version": "0.1.0",
  "description": "Aurelia skeleton with semantic ui",
  "main": "index.js",
  "scripts": {
    "test": "gulp test",
    "start": "gulp serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:lteacher/aurelia-skeleton-semantic.git"
  },
  "author": "Hugo Armstrong <hugo.armstrong@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/lteacher/aurelia-skeleton-semantic",
