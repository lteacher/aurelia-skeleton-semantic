      "aurelia-dependency-injection": "npm:aurelia-dependency-injection@^1.0.0-rc.1.0.0",
      "aurelia-framework": "npm:aurelia-framework@^1.0.0-rc.1.0.1",
      "aurelia-pal": "npm:aurelia-pal@^1.0.0-rc.1.0.0",
